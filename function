#include <iostream>
#include <stdio.h>
#include <windows.h>
#include <conio.h>
#include <math.h>
#include <algorithm>

using namespace std;

int main()
{
	double x, y, z, A, B, h;

	A = 0.5;
	B = 4;
	h = 0.1;

	cout << "\n X \t \t          Y(X) \t\t             Z(X) \n";
	for (x = A; x <= B + 0.1 * h; x += h)
	{
		y = (double)1 / x;
		z = (double)pow(x / 3, 2);
		cout << " x = " << x << "             \t " << y << "              \t " << z << endl;
		 
	}
	return 0;
}

